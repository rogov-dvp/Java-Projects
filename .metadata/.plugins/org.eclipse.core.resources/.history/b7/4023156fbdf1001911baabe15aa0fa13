package objects;

import javax.swing.JFrame;

public class Main {
	public static void main(String[] args) {
		JFrame frame = new JFrame();
		frame.setBounds(10,10,700,600);		//Window size (x,y,width, height); where x,y are top-left coord.
		Gameplay gamePlay = new Gameplay();
		frame.setTitle("Breakout Ball");		//Name of window
		frame.setResizable(false);			//isResizable -> no
		frame.setVisible(true);				//is visible to user -> yes
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);	//Required to close the game
		frame.add(gamePlay);
	}	
}
